      "source": [
        "# A dictionary processed_dataframes is created to store each DataFrame.\n",
        "# For each file, a key is generated based on the filename and year to uniquely identify each DataFrame.\n",
        "# The processed DataFrame is stored in the dictionary, and the data is also saved to\n",
        "# a new Excel file with the original input filename prefixed by filtered_\n",
        "\n",
        "processed_dataframes = {}\n",
        "\n",
        "for input_filename, sheet_name, year in input_files:\n",
        "    # Extract the year and filename base to create a unique key\n",
        "    base_key = input_filename.replace(\".xlsx\", \"\").replace(\"-\", \"_\")\n",
        "    df_key = f\"grants_{year}_{base_key}\"\n",
        "\n",
        "    # Process the data and store the DataFrame in the dictionary\n",
        "    processed_dataframes[df_key] = process_grants_data(input_filename, sheet_name, year)\n",
        "\n",
        "    # Optionally, save each DataFrame to an Excel file\n",
        "    processed_dataframes[df_key].to_excel(f'filtered_{input_filename}', index=False)"
      ]
    },
